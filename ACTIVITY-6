#include <stdio.h>

int checkPrime(int n, int i) {
    if (i == n)
        return 1;

    if (n % i == 0)     
        return 0;

    return checkPrime(n, i + 1);   
}

int main() {
    int n;

    printf("Enter a number: ");
    scanf("%d", &n);

    if (n <= 1)
        printf("Not prime");
    else if (checkPrime(n, 2))
        printf("Prime");
    else
        printf("Not prime");

    return 0;
}
